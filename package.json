{
  "name": "presuposicionalismo.com",
  "type": "module",
  "version": "0.82",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "astro check",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{astro,js,ts,tsx,jsx,mjs,cjs,json,md,mdx}": [
      "prettier --write"
    ],
    "*.{astro,ts,tsx}": [
      "astro check --minimumSeverity warning --noWatch"
    ]
  },
  "dependencies": {
    "@astrojs/check": "0.9.7-beta.1",
    "@astrojs/mdx": "5.0.0-beta.3",
    "@astrojs/rss": "4.0.15-beta.2",
    "@astrojs/sitemap": "3.6.1-beta.2",
    "@astrojs/vercel": "10.0.0-beta.0",
    "@fontsource-variable/urbanist": "^5.2.7",
    "@libsql/client": "^0.15.15",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "6.0.0-beta.6",
    "astro-fuse": "^1.0.4",
    "fuse.js": "^7.1.0",
    "lodash.kebabcase": "^4.1.1",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^11.0.6",
    "postcss-nesting": "^13.0.2",
    "postcss-preset-env": "^10.6.1",
    "prettier": "4.0.0-alpha.12",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.18.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/lodash.kebabcase": "^4.1.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier-plugin-astro": "^0.14.1"
  }
}
